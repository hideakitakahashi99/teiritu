        <span class="text-red-500 required-dot">
         *
        </span>
       <%= f.number_field :participants, in: 1..30, class: 'rounded-lg border-transparent flex-1 appearance-none border border-gray-300 w-full py-2 px-4 bg-white text-gray-700 placeholder-gray-400 shadow-sm text-base focus:outline-none focus:ring-2 focus:ring-purple-600 focus:border-transparent', placeholder: "参加予定人数" %>
     </div>
