        <%= f.label :name, 'お名前', class: 'text-gray-700' %>
        <span class="text-red-500 required-dot">
            *
        </span>
   
    <%= f.text_field :name, class: ' rounded-lg border-transparent flex-1 appearance-none border border-gray-300 w-full py-2 px-4 bg-white text-gray-700 placeholder-gray-400 shadow-sm text-base focus:outline-none focus:ring-2 focus:ring-purple-600 focus:border-transparent', placeholder: "名前" %>
        </div>
        <div class=" relative ">
        <%= f.label :name_kana, 'お名前(ふりがな)', class: 'text-gray-700' %>
        <span class="text-red-500 required-dot">
            *
        </span>
    <%= f.text_field :name_kana, class: 'rounded-lg border-transparent flex-1 appearance-none border border-gray-300 w-full py-2 px-4 bg-white text-gray-700 placeholder-gray-400 shadow-sm text-base focus:outline-none focus:ring-2 focus:ring-purple-600 focus:border-transparent', placeholder: "名前(ふりがな)" %>
        </div>
    </div>
<div class="flex flex-col mb-2">
 <div class=" relative ">
 <%= f.label :address, 'お住まいの住所(市町村までで結構です。)', class: 'text-gray-700' %>
    <span class="text-red-500 required-dot">
        *
    </span>
    <%= f.text_area :address, class: 'rounded-lg border-transparent flex-1 appearance-none border border-gray-300 w-full py-2 px-4 bg-white text-gray-700 placeholder-gray-400 shadow-sm text-base focus:outline-none focus:ring-2 focus:ring-purple-600 focus:border-transparent', placeholder: "住所(市町村までで結構です)" %>
  </div>
</div>
    <div class="flex flex-col mb-2">
        <div class=" relative ">
        <%= f.label :tell, '電話番号', class: 'text-gray-700' %>
        <span class="text-red-500 required-dot">
            *
        </span>
    <%= f.text_field :tell, class: 'rounded-lg border-transparent flex-1 appearance-none border border-gray-300 w-full py-2 px-4 bg-white text-gray-700 placeholder-gray-400 shadow-sm text-base focus:outline-none focus:ring-2 focus:ring-purple-600 focus:border-transparent', placeholder: "電話番号" %>
        </div>
    </div>
    <div class="flex flex-col mb-2">
        <div class=" relative ">
        <%= f.label :email, 'メールアドレス', class: 'text-gray-700' %>
        <span class="text-red-500 required-dot">
            *
        </span>
    <%= f.email_field :email, class: 'rounded-lg border-transparent flex-1 appearance-none border border-gray-300 w-full py-2 px-4 bg-white text-gray-700 placeholder-gray-400 shadow-sm text-base focus:outline-none focus:ring-2 focus:ring-purple-600 focus:border-transparent', placeholder: "メールアドレス" %>

        </div>
    </div>

